bleDetail: false,\n            listItem: {}\n        }\n    },\n    computed:{\n        ...mapGetters({\n            mainList: 'getMainList'\n        }),\n    },\n    methods:{\n        checkShowAll(){\n            this.checkAll = !this.checkAll;\n        },\n        showListDetail(item){\n            this.visibleDetail = true;\n            this.listItem = item;\n            console.log(item);\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.main-list-page{\n    height: calc(100vh - 108px);\n    overflow-y: scroll;\n    .filter-area{\n        text-align: right;\n        .filter{\n            padding: 6px 20px;\n            max-width: 100px;\n            display: inline-block;\n            i{\n                color: #6d5de8;\n            }\n            .fa-check-square{\n                margin-right: 6px;\n            }\n        }\n    }\n    .main-list-item{\n        &:last-child{\n            padding-bottom: 50px;\n        }\n    }\n}\n</style>"],"sourceRoot":""}]}